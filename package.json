{
  "name": "smart-adapter",
  "version": "1.0.0",
  "description": "smart adapter toy project",
  "main": "app.js",
  "engine": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/app.js",
    "backend": "cross-env NODE_ENV=development nodemon server/app.js",
    "deploy": "cross-env NODE_ENV=production nodemon server/app.js",
    "frontend": "npm run start --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\""
  },
  "author": "wonhyeonglee",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "mqtt": "^4.2.6",
    "react-redux": "^5.0.7",
    "saslprep": "^1.0.3",
    "socket.io": "^3.0.5",
    "socket.io-client": "^3.1.0",
    "supports-color": "^7.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "multer": "^1.4.2",
    "nodemon": "^1.19.1"
  }
}
